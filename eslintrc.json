{
  "extends": "eslint:recomended",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "indent": "off",
    "quotes": ["error", "double"],
    "no-console": "off",
    "no-unused-vars": "warn",
    "comma-dangle": "off",
    "brace-style": "off",
    "import/no-commonjs": "off"
  }
}
